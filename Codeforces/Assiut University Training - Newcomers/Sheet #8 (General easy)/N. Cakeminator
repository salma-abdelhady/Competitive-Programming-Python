r, c = map(int,input().split())

mat = []
for _ in range(r):
    row = list(input().strip())
    mat.append(row)

rows = 0
for i in range(r):
    if 'S' not in mat[i]:
        rows += 1

columns = 0
for j in range(c):
    column = [mat[i][j] for i in range(r)]
    if 'S' not in column:
        columns += 1

res = (rows * c) + (columns * r - rows * columns)
print(res)
