from collections import Counter
import math

n = int(input())

arr = []

for i in range(2, int(math.sqrt(n)) + 1):
    if n % i == 0:
        while n % i == 0:
            n //= i
            arr.append(i)
if n > 1:
    arr.append(n)

freq = Counter(arr)

freq_array = list(freq.items())
sz = len(freq_array)

parts = []

for k in freq_array:
    parts.append(f"({k[0]}^{k[1]})")

res = '*'.join(parts)
print(res)        
