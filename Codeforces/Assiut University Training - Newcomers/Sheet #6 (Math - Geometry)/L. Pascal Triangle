n = int(input())

j = 1
lst = []
for i in range(1,n+1):
    if i == 1:
      lst.append([1])
    elif i == 2:
        lst.append([1,1])
    else:
        prev = lst[-1]
        l = [1]
        for k in range(j):
            l.append(prev[k] + prev[k+1])
        j += 1
        l.append(1)
        lst.append(l)

for ls in range(len(lst)):
    print(*lst[ls])
