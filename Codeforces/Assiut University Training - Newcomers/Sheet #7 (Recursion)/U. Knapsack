n , w = map(int,input().split())

weights = []
values = []

for i in range(n):
    wi, vi = map(int,input().split())
    weights.append(wi)
    values.append(vi)

def rec(i, w):
    if i == n or w == 0:
        return 0
    c1 = rec(i+1,w)
    c2 = 0
    
    if w >= weights[i]:
        c2 = rec(i+1,w - weights[i]) + values[i]
    
    return max(c1, c2)

sum = rec(0,w)

print(sum)
