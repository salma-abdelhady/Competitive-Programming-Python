def rec(i = 0, j = 0):
    if i >= n or j >= m:
        return -10**9
    if i == n-1 and j == m-1:
        return mat[i][j]
    c1 = rec(i+1,j)
    c2 = rec(i,j+1)
    return mat[i][j] + max(c1,c2)
    

n, m = map(int,input().split())
mat = []
for _ in range(n):
    row = list(map(int,input().split()))
    mat.append(row)

sum = rec()
print(sum)
